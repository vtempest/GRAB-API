{
  "name": "grab-api.js",
  "description": "Generate Request to API from Browser",
  "version": "0.9.9",
  "author": "vtempest",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vtempest/grab-api"
  },
  "main": "./src/grab-api.js",
  "module": "./dist/grab-api.es.js",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./dist/grab-api.esm.js",
      "require": "./dist/grab-api.js"
    },
    "./types": {
      "types": "./dist/types.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "index.js"
  ],
  "types": "src/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "src/*"
      ]
    }
  },
  "scripts": {
    "build": "vite build",
    "check": "vitest --ui"
  },
  "devDependencies": {
    "@vitest/ui": "^3.1.4",
    "terser": "^5.40.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.1.4"
  },
  "keywords": [
    "api",
    "request",
    "browser",
    "fetch",
    "http"
  ],
  "type": "module"
}
